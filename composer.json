{
    "name": "bobdenotter/conimex",
    "description": "üçõ Extension to Import and Export content as Yaml files",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "require": {
        "php": "^7.2.9 || ^8.0",
        "twig/twig": "^2.0 || ^3.0",
        "league/csv": "^9.6"
    },
    "require-dev": {
        "bolt/core": "^5.0",
        "symplify/easy-coding-standard": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BobdenOtter\\Conimex\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "BobdenOtter\\Conimex\\Extension"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "drupol/composer-packages": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
